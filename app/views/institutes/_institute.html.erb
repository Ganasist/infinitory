<% cache institute do %>
	<div class="institute">
		<table>
    	<% if institute.icon? %>
    			<td><%= link_to image_tag(institute.icon_url(:thumb)).html_safe, institute  %></td>
	        <td><%= link_to "#{institute.name}".html_safe, institute %></td>
      <% else %>
	        <td><%= link_to "#{institute.name}".html_safe, institute %></td>
   		<% end %>
		</table>
	</div>
<% end %>


